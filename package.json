{
  "name": "ats-integration-microservice",
  "version": "1.0.0",
  "description": "Serverless ATS Integration Microservice",
  "main": "index.js",
  "scripts": {
    "start": "serverless offline",
    "deploy": "serverless deploy",
    "deploy:prod": "serverless deploy --stage prod",
