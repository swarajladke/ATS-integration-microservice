    "logs:candidates": "serverless logs -f createCandidate -t",
    "logs:applications": "serverless logs -f getApplications -t"
  },
  "keywords": [
    "ats",
    "serverless",
    "lambda",
    "greenhouse",
    "hiring",
    "recruitment"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.0",
    "serverless-python-requirements": "^6.0.1"
  }
}
