# ATS Integration Microservice - Environment Configuration
# Copy this file to .env and fill in the values

# ATS Provider (currently supported: greenhouse)
ATS_PROVIDER=greenhouse

# Greenhouse API Key (Harvest API)
# Get yours from: https://app.greenhouse.io/configure/dev_center/credentials
ATS_API_KEY=your_greenhouse_api_key_here

# Greenhouse Base URL (default is Harvest API)
ATS_BASE_URL=https://harvest.greenhouse.io/v1

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
